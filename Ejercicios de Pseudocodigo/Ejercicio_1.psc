Algoritmo Ejercicio_1
	//Hacer un algoritmo que declare una variable para guardar el número de horas de estudio, y otra para guardar el nombre 
	Definir horasEstudio Como Real;
	Definir nombre Como Caracter;
	
	horasEstudio <- 9.5;
	nombre <- "juan";
	
	Escribir "las horas de estudio son: ",horasEstudio;
	Escribir "su nombre es: ",nombre;
FinAlgoritmo